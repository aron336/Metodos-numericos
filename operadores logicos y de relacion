#include <stdio.h>
#include <stdlib.h>

int main(){
    int w =9;
	int x =3;
	int y =7;
	int z =-2;
	printf (" %d\n " , (x<y && w>z));
    printf (" %d\n " , (x>=w ^ z==y));	
    printf (" %d\n " , (x<=x || x!=w));
    printf (" %d\n " , (w==9 ^ x==3));
    printf (" %d\n " , (y>z && z<x));
    printf (" %d\n " , (!w!=9));
	system ("pause");
	return 0;
	
}
// parte 2
#include <stdio.h>
#include <stdlib.h>

int main(){
	int P =1;
	int Q =0;
	int R =1;
	int T =0;
	

	printf (" P && R : %d\n  " , (P && R));
	
    printf( " \n");
	printf (" Q || T : %d\n  " , (Q || T));
	 printf( " \n");
	printf (" P && Q || R ^ T : %d\n  " , (P && Q || R ^ T ));
    printf( " \n");
	printf (" !Q && !T : %d\n  " , (!Q && !P));	
    printf( " \n");
    printf (" !!!P : %d\n  " , (!!!P));
	system ("pause");
	return 0;
	
}
